//
// Version Information resources
//

LANGUAGE LANG_ENGLISH, 3
1 VERSIONINFO
 FILEVERSION $MAJORVERSION,$MINORVERSION,$DAYVERSION,$SECONDVERSION
 PRODUCTVERSION $MAJORVERSION,$MINORVERSION,$DAYVERSION,$SECONDVERSION
FILEOS VOS__WINDOWS32
FILETYPE VFT_APP
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "000004b0"
        BEGIN
            VALUE "Author", "Andrew Roach\0"
            VALUE "FileDescription", "Renames image files whilst updating sidecar files\0"
            VALUE "FileVersion", "$MAJORVERSION.$MINORVERSION\0"
            VALUE "InternalName", "exifrensc\0"
            VALUE "LegalCopyright", "Copyright Â©2022\0"
            VALUE "OriginalFilename", "exifrensc.exe\0"
            VALUE "ProductName", "EXIF Rename including sidecar files\0"
            VALUE "ProductVersion", "$MAJORVERSION.$MINORVERSION\0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x0, 1200
    END
END

